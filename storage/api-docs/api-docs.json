                                }
                            }
                        }
                    },
                    "401": {
                        "description": "User should be authorized to get packet information",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "message": {
                                            "type": "string",
                                            "example": "Not authorized"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                },
                "security": [
                    {
                        "bearer": []
                    }
                ]
            }
        },
        "/v1/profile2": {
            "get": {
                "tags": [
                    "profile2"
                ],
                "summary": "Retrieve profile information",
                "description": "Get profile short information",
                "operationId": "profileShow2",
                "responses": {
                    "200": {
                        "description": "Success",
                        "content": {
                            "application/json": {
